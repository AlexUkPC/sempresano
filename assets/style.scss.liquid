	padding: 15px;
	background: $color_3;
	@include border_radius(0);
}

.fancybox-close {
	width: 24px;
	height: 24px;
	display: block;
	border: none;
	background: black;
	border-radius: 5px;
	&:before {
		content: "";
		display: block;
		width: 13px;
		height: 1px;
		background: $color_3;
		position: absolute;
		top: 12px;
		right: 5px;
		@include transform(rotate(45deg));
	}
	&:after {
		content: "";
		display: block;
		width: 13px;
		height: 1px;
		background: $color_3;
		position: absolute;
		top: 12px;
		right: 5px;
		@include transform(rotate(135deg));
	}
	&:hover {
		background: #eee;
		&:before, &:after {
			background: black;
		}
	}
}

.fancybox-nav {
	span {
		width: 36px;
		height: 36px;
		display: block;
		margin: -18px 0 0 0;
		background: #444;
		border: none;
		@include border_radius(50%);
		@include box_shadow(0 0 10px 0 rgba(0, 0, 0, 0.6));
		@include transition(0.3s, background, border-color, color);

		&:before {
			display: block;
			font: 12px/36px "FontAwesome";
			color: $color_4;
			text-align: center;
		}
	}

	&:hover > span {
		background: #666;
	}
}

.fancybox-prev span:before {
	content: "\f053";
	padding: 0 2px 0 0;
}
.fancybox-next span:before {
	content: "\f054";
	padding: 0 0 0 2px;
}

.fancybox_alert {
	padding: 0 !important;
	text-align: center;
	.alert {
		margin: 0;
		padding: 40px;
		font-size: 14px;
	}

	.btn {
		margin: 15px 0 0 0;
		text-decoration: none;
	}
	.btn ~ .btn {
		margin-left: 15px !important;
	}
}


