.wrap_triger {
	position: relative;
	padding-right: 25px;

	&:after {
		position: absolute;
		content: '';
		right: 0;
		top: 50%;
		cursor: pointer;
		-webkit-mask-repeat: no-repeat;
		mask-repeat: no-repeat;
		-webkit-mask-position: 50%;
		mask-position: 50%;
		background-color: #6C6E71;
		transition: all 0.3s ease-in-out;
		width: 12px;
		height: 12px;
		-webkit-mask-image: url('data:image/svg+xml;utf8,<svg width="12" height="12" viewBox="0 0 12 12"  xmlns="http://www.w3.org/2000/svg"><path d="M7 0H5V5H0V7H5V12H7V7H12V5H7V0Z"/></svg>'); mask-image: url('data:image/svg+xml;utf8,<svg width="12" height="12" viewBox="0 0 12 12" xmlns="http://www.w3.org/2000/svg"><path d="M7 0H5V5H0V7H5V12H7V7H12V5H7V0Z" /></svg>');
		transition: all 0.3s ease-in-out;
		@include transition(.3s,transform);

	}
	&.open:after {
		position: absolute;
		content: '';
		right: 0;
		top: 50%;
		cursor: pointer;
		-webkit-mask-repeat: no-repeat;
		mask-repeat: no-repeat;
		-webkit-mask-position: 50%;
		mask-position: 50%;
		background-color: #171717;
		transition: all 0.3s ease-in-out;
		width: 12px;
		height: 3px;
		-webkit-mask-image: url('data:image/svg+xml;utf8,<svg width="12" height="2" viewBox="0 0 12 2" xmlns="http://www.w3.org/2000/svg"><rect width="12" height="2" /></svg>'); mask-image: url('data:image/svg+xml;utf8,<svg width="12" height="2" viewBox="0 0 12 2" xmlns="http://www.w3.org/2000/svg"><rect width="12" height="2" /></svg>');
		transition: all 0.3s ease-in-out;
		@include transition(.3s,transform);
	}
}
