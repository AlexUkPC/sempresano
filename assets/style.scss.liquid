// 6.5. BREADCRUMBS =================================================================================================================================
.breadcrumbs {
	padding: 15px 0 31px 0;
	ul {
		list-style: none;
		padding: 0;
		font-size: 0;
		line-height: 0;
	}

	li {
		display: inline-block;
		padding: 21px 8px 0 0;
		span {
			position: relative;
		}
	}
	li ~ li {}
	li ~ li:before {
		content: "/";
		margin: 0 10px 0 0;
		font-size: 17px;
		font-weight: lighter;
	}
	li:last-child {
		span {
			font-size: 14px;
			font-weight: 400;
			color: #262629;
		}
	}

	a,
	span {
		font-size: 14px;
		line-height: 1em;
		letter-spacing: -0.07em;
	}

	span {
		text-transform: capitalize;
	}
	a {
		position: relative;
		font-size: 14px;
		font-weight: 400;
		color:#676A6D;
			&:hover{
				color: #262629;
			}
	}
}



// 6.6. BUTTON FILTERS ==============================================================================================================================
.button_filters {
	margin: 5px 0 30px;
	ul {
		font-size: 0;
	}
	li {
