		a, span {
			font-size: $base_font_size;
			height: 44px;
			line-height: 44px;
			padding: 0 30px;
			font-weight: bold;
			text-align: center;
			display: inline-block;
			color:$heading_font_color;
		}
		&:hover {
			a {
				border-color: $color_1;
			}
		}
	}

	.pagination_prev {
		a {
			vertical-align: middle;
		}
		i {
			font-size: 34px;
			margin: 0 5px 0 0;
		}
	}
	.pagination_next {
		a {
			vertical-align: middle;
		}
		i {
			font-size: 34px;
			margin: 0 0 0 5px;
		}
	}
	.pagination_current span {
		color: $color_2;
		transition: 0.5s;
		position: relative;
		display: flex;
		align-items: center;
		justify-content: center;
		&:before {
			content: '';
			display: block;
			position: absolute;
			bottom: 7px;
			width: 14px;
			height: 3px;
			background: $links_color_hover;
			transition: 0.5s;
		}


	}
