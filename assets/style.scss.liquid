// 7.7. IMAGE WITH TEXT OVERLAY =====================================================================================================================
.section_image-with-text-overlay {
	margin: 0;
	position: relative;
	.section_wrap {overflow: hidden;position: relative;}
	.container .section_wrap {border: 1px solid $border_color;}

	.flexible_block__small {padding: 24.414062% 0 0 0;}
	.flexible_block__medium {padding: 31.25% 0 0 0;}
	.flexible_block__large {padding: 40.365% 0 0 0;}


	.img_placeholder__small {padding: 24.414062% 0 0 0;}
	.img_placeholder__medium {padding: 31.25% 0 0 0;}
	.img_placeholder__large {padding: 40.365% 0 0 0;}


	.parallax_layer.img_placeholder__small {padding: 43% 0 0 0;}
	.parallax_layer.img_placeholder__medium {padding: 53.711% 0 0 0;}
	.parallax_layer.img_placeholder__large {padding: 60% 0 0 0;}
	
	.parallax_layer_2.img_placeholder__small {padding: 43% 0 0 0;}
	.parallax_layer_2.img_placeholder__medium {padding:39.711% 0 0 0;}
	.parallax_layer_2.img_placeholder__large {padding: 60% 0 0 0;}
	.parallax_layer_2 {background: none;background-repeat: no-repeat;}


	.img_placeholder__wrap {
		position: absolute;
		top: 0;
		right: 0;
		left: 0;
		z-index: 1;
		background-position: 50% 50%;
		@include bg_size( cover );
	}

	.section_txt {
		padding: 15px;
		text-align: left;
		position: absolute;
		top: 0;
		right: 0;
		bottom: 0;
		left: 0;
		z-index: 10;
		display: flex;
		align-items: center;
		.container {display: flex;text-align: left;}
      	&.align_left.full_width {
          bottom: 13%;
        }
		&.align_right {
          .full_width {
            bottom: 5%;
            .btn {
              margin-top: 20px;
              bottom: 0;
            }
          }
          .container {justify-content: flex-end;text-align: left;}
        }
		&.align_center .container {
			justify-content: center;
			text-align: center;
        h2 {
            margin-bottom: 20px;
        }
			margin-top: -40px;
			@media (min-width: 992px) {
				margin-top: -94px;
			}
		}
	}
	.caption_text {
		max-width: 65%;
		padding: 50px 0;
	}

	h2 {
		font-size: 48px;
		text-transform: uppercase;
		font-weight: bold;
		margin: 15px 0 25px;
    }
	h4 {
		font-size: 26px;
		text-transform: uppercase;
		font-weight: bold;
		margin-bottom: 28px;
		margin-top: 25px;
		line-height: 1em;
	}
	h4 + h4 {
		margin-bottom: 5px;
	}
	h5 {
		font-size: 18px;
		text-transform: uppercase;
		font-weight: 600;
		letter-spacing: -0.01em;
	}
	p {
		margin: 10px 0 0 0;
		color: $heading_font_color;
	}
	.btn {
		font-size: 14px;
		padding: 13px 15px;
		min-width: 170px;
		margin-top: 8px;
	}
	.btn svg {
		vertical-align: middle;
		margin-top: -2px;
		margin-left: 6px;
		margin-right: 0;
	}
	.inverted {
		h4, h5, p {
			color: $color_3;
		}
      	.btn {
            background: {{ settings.alt_button_color }};
            border: 2px solid {{ settings.alt_button_border_color }};
            span {
              color: {{ settings.alt_button_text_color }};
            }
          	svg path {
              fill: {{ settings.alt_button_text_color }};
            }
            &:hover {
              	background: {{ settings.alt_button_hover_color }};
            	border-color: {{ settings.alt_button_border_color }};
              	span {
                  color: {{ settings.alt_button_hover_text_color }};
                }
              	svg path {
                  fill: {{ settings.alt_button_hover_text_color }};
                }
        	}
          
        }
	}
.align_left.full_width{
	.caption_text {
		padding: 50px 0;

	}
}
	.full_width {
		.caption_text {
			max-width: 100%;
		}
		h3 {margin: 0;}
		h2 p {
			font-size: 72px;
			line-height: 79px;
			margin: 0;
			text-transform: uppercase;
			font-weight: 500;
			span {
				color: $color_2;
			}
		}

		h4 {
			font-size: 32px;
			font-weight: 400;
			margin: 25px 0 11px;
		}
		h5 {
			font-size: 30px;
			font-weight: 500;
			text-transform: none;
			color: $color_2;
			margin: 10px 0 14px;
		}
		p {
			font-size: 22px;
			line-height: 1em;
			font-weight: 400;
			margin: 0;
		}
		.btn {
			margin-top: 50px;
			padding: 13px 0;
			position: relative;
			bottom: 4px;
			span {
				position: relative;
				left: 6px;
				border: none;
			}
		}
	}

	.boxed {
		.container {
			width: 100%;
		}
	}
}


// 7.8. SLIDESHOW ===================================================================================================================================
.index-section-slideshow {
	display: flex;
	justify-content: center;
}

.section_slideshow {
	margin: 0;
	background: $color_3;
	position: relative;

	.swiper-slide,
	.slideshow_image {
		width: 100%;
		min-width: 100%;
	}
	.image_slide {
		line-height: 0;
	}

	.img_placeholder__wrap {
		background-position: 0 0;
		-webkit-background-size: 100% auto;
		background-size: 100% auto;
	}
	.img_placeholder__small {
		padding: 33.15% 0 0 0;
	}
	.img_placeholder__medium {
		padding: 38.6745% 0 0 0;
	}
	.img_placeholder__large {
		padding: 44.199% 0 0 0;
	}

	.slide_caption__subheader {
		font-size: 16px;
		margin: 0;
	}

	h2 {
		font-size: 60px;
		line-height: 1em;
		margin: 15px 0 18px;
		text-transform: uppercase;
	}
	p {
		line-height: 1em;
	}
	
	.inverted {
		h2 {
			color: $color_3;
		}
		p {
			color: $color_3;
		}
	}
	.swiper_btn__prev {
		left: 43px;
	}
	.swiper_btn__next {
		right: 43px;
	}
	.slider_btn {
		margin: 14px 2px 0;
		min-width: 126px;
	}
	.slide_caption {
		display: block;
		padding: 40px 80px;
		color: inherit;
		text-align: center;
		position: absolute;
		top: 0;
		right: 0;
		bottom: 0;
		left: 0;
		z-index: 100;
		//text-shadow: 1px 1px 0 rgba(0, 0, 0, 0.6);

		display: flex;
		align-items: center;
	}
	.position_left {
		justify-content: flex-start;
		text-align: left;
	}
	.position_center {
		justify-content: center;
	}
	.position_right {
		justify-content: flex-end;
		text-align: right;
	}
	.swiper_pagination {z-index: 1;}
	.swiper_pagination.swiper-pagination-progressbar {
		bottom: 0px;
	}
	.slide_video {
		.mbYTP_wrapper {
			.playerBox {

			}
		}
		.inlinePlayButton {display: none !important;}
	}
	.tablet, .mobile  {
		.swiper_pagination {display: none;}
	}
}

.swiper-container-horizontal > .swiper-pagination-bullets .swiper-pagination-bullet {
	vertical-align: middle;
}

.inline-YTPlayer {
	position: absolute !important;
	max-width: 1810px !important;
	width: 100%;
	top: 0;
	bottom: 0;
}



