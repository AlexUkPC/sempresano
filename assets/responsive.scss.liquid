// 6.3. MAIN PRODUCT LISTING ========================================================================================================================
.product_listing__main {
	.product_item {
		&:hover {
			.product_info {
				margin-top: 0px;
			}
		}
	}
	.product_info .add_to_cart_form {opacity: 1;}
}
.product_listing__main {
	.product_item_wrap {
		width: 100%;
		padding-top: 30px;
		padding-bottom: 30px;

		.product_item {
			display: flex;
			position: relative;
			margin: 0;
			flex-direction: column;

			&:after {
				content: '';
				position: absolute;
				bottom: -30px;
				left: 0;
				right: 0;
				height: 1px;
				background: {{ settings.border_color }};
			}

			.product_img_wrap {
				width: 100%;
				position: static;
			}
			.product_img {
				width: auto;
				img {
					display: none;
					min-width: 100%;
				}
				.image_2 {
					display: block;
				}
			}
			.bade_wrap {
				top: 5px;
				left: 10px;
			}
			.product_info {
				width: 100%;
				text-align: center;
				padding-top: 28px;

				.product_vendor {
					display: block;
					margin: 0;
					line-height: 1em;
				}
				.product_name {
					font-size: 18px;
					margin: 5px 0;
					padding: 0;
				}
				.product_price {
					padding: 0;
				}
				.main_price {
					font-size: 18px;
				}
				.product_desc {
					display: none;

				}
			}
			
		}
		.product_links {
			opacity: 1;
			display: flex;
			justify-content:center;

			.add_to_cart_form {
				position: static;
				margin: 0;
				.btn {
					padding: 8px 10px;
					margin: 0;
				}
			}
			.product_links__subbutton {
				position: relative;
				top: 0;
				right: auto;
				margin: 0;
			}

		}


		.quick_view_btn, 
		.wishlist_btn {
			display: inline-block;
			vertical-align: middle;
			width: 43px;
			height: 43px;
			line-height: 37px;
		}
	}
	.quick_view_btn {
		display: none;
	}

	.product_item {
		 
		.wishlist_btn {
			width: 43px;
			height: 43px;
			line-height: 37px;
			margin-bottom: 0;
			display: inline-block;
			line-height: 37px;
		}

		.quick_view_btn {
			display: none;
		}
	}

	.product_info {
		.product_color {
			display: none;
		}
	}
	.product_badge {
		padding: 0 6px;
		font-size: 10px;
		line-height: 2em;
	}
	.product_links {
		opacity: 1;
		display: flex;
		justify-content: center;
		margin-top: 12px;
		&.product_links_listing{display: none;}
		.product_links__subbutton {
			position: static;
		}
		.add_to_cart_form {
			position: static;
			.btn {
				margin: 0;
				padding: 8px 9px;
			}
		}
		.btn_options, 
		.btn-cart {
			padding: 10px;
			span {
				display: none;
			}
			svg {
				margin-right: 0;
			}
		}

		.wishlist_btn {
			margin-bottom: 0;
			display: inline-block;
			vertical-align: middle;
			padding: 0;
			border-radius: 0;
			border: 2px solid {{ settings.button_color }};
			background: {{ settings.button_color }};
			color: {{ settings.color_3 }};
			svg {
					fill: transparent;
					stroke: {{ settings.color_3 }};
				}
			&:hover {
				border: 2px solid {{ settings.color_1 }};
				background: {{ settings.color_1 }};
				color: {{ settings.color_3 }};
				svg {
					fill: transparent;
					stroke: {{ settings.color_3 }};
				}
			}
			svg {
				display: inline-block;
			}
			span {
				display: none;
			}
		}
	}
}
.product_listing__list {
	.product_links.product_links_listing .quick_view_btn span {
		text-transform: uppercase;
	}
	.product_img{
		padding: 10px;
	}
	.product_links .product_links__subbutton {
		.quick_view_btn {
			width: auto;
			margin: 0;
			margin-left: 10px;
		}
	}
	.product_item_wrap{
		.product_links{display: none;}
		.product_links.product_links_listing{
			margin-top: 0;
			display: flex;order: 3;
		a{margin: 0;}
		}
		.product_item {
			flex-direction: row;
			.product_info {
				padding-top: 0;
				display: flex;
				flex-direction: column;
				align-items: flex-start;
				.product_desc {
					display: flex;
					text-align: left;
				}
			}
		}
	}
}


